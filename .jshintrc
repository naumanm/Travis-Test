{
  "node": true,
  "predef": ["angular", "_", "WS", "Autodesk"],
  "globals": {
    "$": false,
    "jasmine": false,
    "spyOn": false,
    "it": false,
    "xit": false,
    "console": false,
    "describe": false,
    "expect": false,
    "beforeEach": false,
    "afterEach": false,
    "ActiveXObject": false,
    "inject": false,
    "module": false
  },
  "bitwise": false,
  "immed": true,
  "newcap": false,
  "noarg": true,
  "noempty": true,
  "nonew": true,
  "trailing": true,
  "maxlen": 300,
  "boss": true,
  "eqnull": true,
  "expr": true,
  "globalstrict": true,
  "laxbreak": true,
  "loopfunc": true,
  "sub": true,
  "undef": true,
  "indent": 2,
  "browserify": true,
  "curly": false,
  "eqeqeq": true,
  "freeze": true,
  "latedef": "nofunc",
  "maxdepth": 5,
  "maxstatements": 50,
  "strict": false,
  "unused": true,
  "maxparams": 20
}
